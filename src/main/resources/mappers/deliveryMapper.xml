<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.quiz.lesson05.delivery.mapper.DeliveryMapper">
 	<select id="selectStores" parameterType="map" resultType="com.quiz.lesson05.delivery.domain.Store">
 		SELECT 
 			`name`
 			,`phoneNumber`
 			,`address`
 		FROM `store`
 	</select>
 
 </mapper>