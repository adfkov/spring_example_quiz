<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.quiz.lesson03.dao.UsedGoodsDAO">
	<select id="selectUsedGoodsList" resultType="com.quiz.lesson03.quiz01.model.UsedGoods">
		SELECT
			`id`  
		,`realtorId`
		,`address`  
		,`area`  
		,`type` 
		,`price `
		,`rentPrice `
		,`createdAt` 
		,`updatedAt`
		FROM
			`real_estate`
	</select>
</mapper>
